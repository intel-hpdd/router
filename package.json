{
  "name": "intel-router",
  "version": "5.0.1",
  "main": "dist/source/get-router.js",
  "description": "A simple utility to route requests to actions.",
  "scripts": {
    "test:dev": "npm run prepublish && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "eslint": "eslint ./",
    "jscs": "jscs .",
    "prepublish": "./bin/build.sh",
    "cover:dev": "npm run cover -- --reporter=html",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t"
  },
  "nyc": {
    "include": [
      "/"
    ],
    "exclude": [
      "test"
    ]
  },
  "pre-commit": [
    "jscs",
    "eslint",
    "test:dev"
  ],
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "path-to-regexp": "1.2.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.6.5",
    "babel-eslint": "5.0.0",
    "babel-plugin-__coverage__": "0.1111.1",
    "babel-plugin-transform-object-assign": "6.5.0",
    "babel-plugin-transform-strict-mode": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "eslint": "2.2.0",
    "eslint-plugin-flow-vars": "0.2.1",
    "intel-jasmine-junit-reporter": "1.0.0",
    "intel-jasmine-n-matchers": "2.1.1",
    "jasmine": "2.4.1",
    "jscs": "2.11.0",
    "nyc": "6.1.1",
    "pre-commit": "1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.intel.com/IML/router.git"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  }
}
